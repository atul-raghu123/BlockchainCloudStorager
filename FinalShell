sudo ./bringupnetwork.sh
pushd ../chaincode/docstore/cloudchaincode
GO111MODULE=on go mod vendor
popd
sudo docker exec cli peer lifecycle chaincode package server.tar.gz --path /opt/gopath/src/github.com/hyperledger/fabric-samples/chaincode/docstore/cloudchaincode --label scc
sudo docker exec cli peer lifecycle chaincode install server.tar.gz
sudo docker exec -e CORE_PEER_MSPCONFIGPATH=/opt/gopath/src/github.com/hyperledger/fabric/peer/crypto/peerOrganizations/org2.example.com/users/Admin@org2.example.com/msp -e CORE_PEER_ADDRESS=peer0.org2.example.com:9051 -e CORE_PEER_LOCALMSPID="Org2MSP" -e CORE_PEER_TLS_ROOTCERT_FILE=/opt/gopath/src/github.com/hyperledger/fabric/peer/crypto/peerOrganizations/org2.example.com/peers/peer0.org2.example.com/tls/ca.crt cli peer lifecycle chaincode install server.tar.gz
